	"paths": {
		"/healthcheck": {
			"get": {
				"summary": "Healthcheck route",
				"description": "This is route is for any orchestrator as Kubernetes or other devs, to check if the API service is alive",
				"tags": ["HEALTH"],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"$ref": "#/components/schemas/HealthcheckResponse"
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"HealthcheckResponse": {
				"type": "object",
				"properties": {
					"message": {
						"type": "string"
					}
				}
			}
		}
